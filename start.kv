MDScreen:
    name: "start"
    MDFloatLayout:
        md_bg_color: 1, 1, 1, 1
        MDLabel: 
            text: "Math\nSolver"
            font_name: "Mogul"
            font_size: "55sp"
            pos_hint: {"center_x": .2, "center_y": .7}
            color: 1, 1, 1, 1
            line_height: 1.1
        Button:
            size_hint: .55, .12
            pos_hint: {"center_x": .28, "center_y": .83}
            background_color: 0, 0, 0, 0
            on_release:
                root.manager.current = "select_sign"
            Image:
                center_x: self.parent.center_x
                center_y: self.parent.center_y
                source: 'Images/level-1.png'
